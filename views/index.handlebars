<div class='container' id="wrapper">

  <div class="jumbotron">
    <h1 class="display-3">Game Spot New Scrapper</h1>
  </div>

  <div id="articles">

    {{#each article}}
      <div class='row'>
        <div class="panel panel-default">
          <div class="panel-heading">
            {{this.title}}
            <a class="btn btn-primary" href="#" role="button">Article Notes</a>
            <a class="btn btn-danger" href="#" role="button">Delete From Saved</a>
          </div>
          <div class="panel-body">
            {{this.headline}}
          </div>
        </div>
      </div>
    {{/each}}
    
  </div>

</div>